//-
    name: searchform
    description: Markup for the searchform block
    arguments:
        - {object} props - block properties

mixin searchform(data = {})
    .searchform&attributes(attributes)
        .searchform__cower
            form.searchform__form
                input.searchform__inputSearch(type="search" placeholder="search")
                button.searchform__button!= data.button

